<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@id/operationsFragment">


    <fragment
        android:id="@+id/operationsFragment"
        android:name="selitskiyapp.hometasks.financialassistant.presentation.fragments.OperationsFragment"
        android:label="fragment_operations"
        tools:layout="@layout/fragment_operations" >
        <action
            android:id="@+id/to_addOperationBottom"
            app:destination="@id/addOperationBottom" />
        <action
            android:id="@+id/to_editOperationBottom"
            app:destination="@id/editOperationBottom" />
    </fragment>
    <fragment
        android:id="@+id/moneyHolderFragment"
        android:name="selitskiyapp.hometasks.financialassistant.presentation.fragments.MoneyHolderFragment"
        android:label="fragment_money_holder"
        tools:layout="@layout/fragment_money_holder" />
    <dialog
        android:id="@+id/editOperationBottom"
        android:name="selitskiyapp.hometasks.financialassistant.presentation.fragments.EditBottom"
        android:label="EditBottom"
        tools:layout="@layout/bottom_edit_operation" />
    <dialog
        android:id="@+id/addOperationBottom"
        android:name="selitskiyapp.hometasks.financialassistant.presentation.fragments.AddDebitBottom"
        android:label="AddDebitBottom"
        tools:layout="@layout/bottom_add_operation" />

</navigation>